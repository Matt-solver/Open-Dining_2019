<style type="text/css">
@-webkit-keyframes typing {
    from {
        width: 0
    }
    to {
        width: 16.5em
    }
}
@-webkit-keyframes blink-caret {
    from, to {
        border-color: transparent
    }
    50% {
        border-color: black
    }
}
body {
    font-family: Consolas, monospace;
}
h1 {
    font-size: 150%;
    width: 16.5em;
    white-space: nowrap;
    overflow: hidden;
    border-right: .1em solid black;
    -webkit-animation: typing 1.7s steps(31, end), blink-caret 1s step-end infinite;
}
</style>
